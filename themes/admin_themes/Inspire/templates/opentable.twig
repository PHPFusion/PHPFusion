<div class="opentable">
    <div class="navbar navbar-subheader">
        <div class="container p-0">
            <div class="navbar-main">
                <div class="breadcrumb-wrapper">
                    {{ breadcrumbs|raw }}
                </div>
            </div>
            <div class="m-l-a admin-actions" style="align-self: center" aria-label="Admin Helpers" role="button">
                {% if links %}
                    {% for link in links %}
                        <a href="{{ link.link|raw }}" class="btn btn-default">{{ link.title|raw }}</a>
                    {% endfor %}
                {% endif %}
            </div>
        </div>
    </div>
    {% if links %}
        <div class="admin-helper">
            <ul class="helper-content">
                {% for link in links %}
                    <li id="{{ link.id }}" class="helper-options {{ link.class }}" style="display:none;">
                        {{ link.content|raw }}
                    </li>
                {% endfor %}
            </ul>
        </div>
    {% endif %}
    <div class="content">
        {% if title %}
            <h1 class="m-0">{{ title }}</h1>
        {% endif %}
